<view class="alice _div"><view class="header _div"><view class="_div"><label>名字:</label><label class="_span">{{name}}</label></view><view class="_div"><label>生日:</label><label class="_span"><block wx:if="{{year}}">{{year+"岁"}}</block>{{''+(month?month:1)+'月'}}</label></view><view class="_div"><label>地址:</label><label class="_span">{{provinceName+city+county}}</label></view></view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item _div" id="{{'item-'+index}}"><view class="item-title _div">{{index+1+"."+item.name}}</view><view data-event-opts="{{[['tap',[['setChecked',['$0',1,index],[[['list','',index]]]]]]]}}" class="{{['check _div',(item.checked===1)?'active':'']}}" bindtap="__e">是</view><van-divider vue-id="{{'1e5e5e5e-1-'+index}}" customStyle="margin-top:0rpx;margin-bottom:0rpx;" data-com-type="wx" bind:__l="__l"></van-divider><view data-event-opts="{{[['tap',[['setChecked',['$0',0,index],[[['list','',index]]]]]]]}}" class="{{['check _div',(item.checked===0)?'active':'']}}" bindtap="__e">否</view></view></block><view class="foot _div"><van-progress vue-id="1e5e5e5e-2" color="#8063c4" percentage="{{count/list.length*100}}" stroke-width="{{2}}" custom-class="foot-progress" show-pivot="{{false}}" data-com-type="wx" bind:__l="__l"></van-progress><view class="foot-submit _div">{{'共计'+list.length+"题　　"+count+"/"+list.length+'题'}}<view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="foot-button _div" bindtap="__e">提交</view></view></view></view>