<view class="pt44 _div">
  <view class="disflex jsbet bgfff lh30 pl16 pr15 pt5 pb5 posfix top0 left0 w100p borderbox _div">
    <view class="bgf5f6 bradius3 disflex pl15 flex1 _div"><label class="disflex pl10 jscen align-cen _span">
        <image class="w16 h16 menu_more posab _img"
          src="https://yyker-one-stand.oss-cn-beijing.aliyuncs.com/yimai_photos/user/search.png" alt></image>
      </label><input class="pl15 pha8 fs14 lh34 h34 w100p c38" type="text" placeholder="请输入预约名称搜索"
        data-event-opts="{{[['confirm',[['search',['$event']]]],['input',[['__set_model',['','key','$event',[]]]]]]}}"
        value="{{key}}" bindconfirm="__e" bindinput="__e" /></view><label
      data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="fs16 cblue lh34 pl15 _span"
      bindtap="__e">搜索</label>
  </view>
  <block wx:if="{{goodsList.length===0}}">
    <view class="pt80 textc _div">
      <block wx:if="{{haveNoSearch}}">
        <image class="w170 h170 _img"
          src="https://yyker-one-stand.oss-cn-beijing.aliyuncs.com/one-www/photo/20190520/1558319783804.png" alt>
        </image>
      </block>
      <block wx:else>
        <image class="w170 h170 _img"
          src="https://yyker-one-stand.oss-cn-beijing.aliyuncs.com/one-www/photo/20190521/1558429867593.png" alt>
        </image>
        <view class="fs12 lh32 ca8 _div">没有搜索到此类预约哦</view>
      </block>
    </view>
  </block>
  <block wx:else>
    <view class="bgf5f6 pl15 pr15 pb23 _div">
      <block wx:for="{{goodsList}}" wx:for-item="v" wx:for-index="k" wx:key="k">
        <get-phone-number-goods vue-id="{{'2ae20cdc-1-'+k}}" goodsType="product" goodInfo="{{v}}"
          data-event-opts="{{[['^next',[['toProdDetail',['$0'],[[['goodsList','',k,'productsId']]]]]]]}}"
          bind:next="__e" bind:__l="__l"></get-phone-number-goods>
      </block>
    </view>
  </block>
  <block wx:if="{{nodata&&goodsList.length>0}}">
    <view class="textc lh42 fs12 ca8 bgf5f6 _div">- 到底了，看看其他的吧 -</view>
  </block>
</view>