	    <scroll-view scroll-y>
	      <!--Tab布局--> 
	      <view class='title'>
	        <view  data-event-opts="{{[['tap',[['titleClick',['0']]]]]}}" bindtap="__e" class="{{0 == currentIndex ? 'title-sel-selected' : 'title-sel'}}"  >
