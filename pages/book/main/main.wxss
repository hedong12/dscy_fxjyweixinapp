.type1-item {
	width: calc(20% - 14rpx);
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	margin: 0 7rpx;
	display: inline-block;
	font-size: 30rpx;
	color: black;
	border-radius: 10rpx;
	text-align: center;
	height: 64rpx;
	background-color: #F2F2F2;
	line-height: 64rpx
}

.activenav{
	color: #0D9784;
	background-color: #ACDED3;
}