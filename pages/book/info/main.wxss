page,
.alice {
	width: 100%;
	height: 100%
}

.key {
	position: fixed;
	top: 0;
	z-index: 999999999999999;
	background-image: url(https://yyker-one-stand.oss-cn-beijing.aliyuncs.com/yimai_photos/user/user-book-key.gif);
	-webkit-transform: translate(25%);
	-ms-transform: translate(25%);
	transform: translate(25%);
	width: 58rpx;
	height: 110rpx;
	background-size: 100% 100%;
	background-repeat: no-repeat
}

.overlayShow {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	-ms-flex-align: center;
	align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	-ms-flex-pack: center;
	justify-content: center;
	height: 100%
}

.overlayShow image {
	width: 695rpx;
	height: 799rpx
}

.disabled-swiper-slide {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 700rpx;
	z-index: 999999999999
}

.book-share {
	width: 99rpx;
	height: 87rpx;
	background-image: url(https://yyker-one-stand.oss-cn-beijing.aliyuncs.com/yimai_photos/user/user-book-share.png);
	background-size: 100% 100%;
	position: absolute;
	top: 10rpx;
	right: 35rpx;
	background-color: rgba(0, 0, 0, 0);
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	margin: 0;
	padding: 0
}

.control {
	position: relative
}

.control .book-back {
	width: 45rpx;
	height: 45rpx;
	position: absolute;
	top: calc(50% - 22rpx);
	left: calc(50% - 35rpx - 40rpx - 89rpx)
}

.control .book-forward {
	width: 45rpx;
	height: 45rpx;
	position: absolute;
	top: calc(50% - 22rpx);
	left: calc(50% + 40rpx + 89rpx)
}

.control .book-suspend {
	width: 80rpx;
	height: 80rpx;
	position: absolute;
	top: calc(50% - 40rpx);
	left: calc(50% - 40rpx)
}

.content-body {
	color: #1d1d1d;
	font-size: 28rpx;
	word-break: break-all
}

.content-title {
	font-weight: 700;
	color: #1d1d1d;
	font-size: 32.93rpx;
	text-align: center;
	margin-bottom: 34rpx
}

.content {
	padding-top: 64rpx;
	padding-bottom: 64rpx;
	padding-left: 26rpx;
	padding-right: 26rpx;
	background-color: #fff;
	-webkit-box-sizing: border-box;
	box-sizing: border-box
}

