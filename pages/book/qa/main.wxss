page,
.alice {
	width: 100%;
	height: 100%;
	overflow: hidden
}

.overlay {
	margin-top: 170rpx;
	text-align: center
}

.overlay-button {
	margin: 0 auto;
	background-color: #41AB9F;
	border-radius: 34px;
	color: #fff;
	font-size: 34rpx;
	width: 355rpx;
	height: 74rpx;
	line-height: 74rpx;
	text-align: center;
	margin-top: 30rpx;
}

.overlay ._img {
	width: 460rpx;
	height: 460rpx;
	margin: 0 auto
}

.card-icon {
	width: 100%;
	height: 273rpx;
	background-image: url(http://yyker-one-stand.oss-cn-beijing.aliyuncs.com/yimai_photos/login/dscy_time.png);
	background-size: 100% 100%;
	background-position-x: center;
	background-position-y: center;
	background-repeat: no-repeat;
	position: absolute;
	top: -188.5rpx;
}

.lv-item{
	color: #41AB9F;
}

.timu{
	width: 85%;
	margin: 0 auto;
	font-size: 34rpx;
}

.card-title {
	margin-bottom: 25rpx;
	color: #000;
	text-align: center
}



.timutitle{
	    float: left;
	    width: 50rpx;
	    height: 50rpx;
	    background-color: #A7A7A7;
	    line-height: 18rpx;
	    color: white;
	    border-radius: 100%;
		margin: 16rpx 30rpx 0;
		text-align: center;
}


.card-qa {
	height: 79rpx;
	line-height: 79rpx;
	border-radius: 10rpx;
	position: relative;
	width: calc(100% - 140rpx);
	margin: 27rpx auto;
	background-color: #F1F1F1;
	color: black
}


.active-title{
	background-color: #40AA9E;
}

.cardactive{
	background-color: #ACDED3
}


.card {
	padding-top: 80rpx;
	padding-bottom: 84rpx;
	background-color: #fff;
	position: absolute;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	top: 191rpx;
	width: calc(100% - 100rpx);
	left: 50rpx;
	border-radius: 50rpx;

}

.alice {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%
}

.alice>._img {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	-webkit-filter: blur(10px);
	filter: blur(10px)
}
