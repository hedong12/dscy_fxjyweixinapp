.card {
	-webkit-box-shadow: 0 6rpx 21rpx 0 rgba(6, 0, 1, .22);
	box-shadow: 0 6rpx 21rpx 0 rgba(6, 0, 1, .22);
	padding-top: 48rpx;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	position: relative;
	width: calc(100% - 42rpx);
	margin-left: auto;
	margin-right: auto;
	border-radius: 20rpx;
	background: -webkit-gradient(linear, left top, right top, from(#c1a168), to(#ead9b3));
	background: -o-linear-gradient(left, #c1a168, #ead9b3);
	background: linear-gradient(90deg, #c1a168, #ead9b3);
	margin-top: 38rpx;
	margin-bottom: 29rpx
}

.userinfo-membership-bg {
	background: -webkit-gradient(linear, right top, left top, from(#baa28f), to(#feeddc));
	background: -o-linear-gradient(right, #baa28f 0, #feeddc 100%);
	background: linear-gradient(270deg, #baa28f 0, #feeddc);
	-webkit-transform: skew(-30deg);
	-ms-transform: skew(-30deg);
	transform: skew(-30deg);
	display: inline-block;
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
