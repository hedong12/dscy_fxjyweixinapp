<view class="tabbar-container">
	
  <view class="tabbar-item {{active == 0 ? 'tabbar-select':''}}" bindtap="switchJump" data-url="../../pages/index/main" data-active="0">
    <view class="tabbar-image" >
      <image src="{{active == 0 ? 'https://yyker-one-stand.oss-cn-beijing.aliyuncs.com/yimai_photos/login/dscy_computer.png':'https://yyker-one-stand.oss-cn-beijing.aliyuncs.com/yimai_photos/login/dscy_computer2.png'}}"></image>
    </view>
    <view class="tabbar-text">首页</view>
  </view>
  
  
  <view class="tabbar-item {{active == 1 ? 'tabbar-select':''}}" bindtap="switchJump" data-url="../../pages/Product/main" data-active="1">
    <view class="tabbar-image">
      <image style="width: 55rpx;height: 28rpx;margin-top: 14rpx;" src="{{active == 1 ? 'https://yyker-one-stand.oss-cn-beijing.aliyuncs.com/yimai_photos/login/dscy_appointment.png':'https://yyker-one-stand.oss-cn-beijing.aliyuncs.com/yimai_photos/login/dscy_appointment2.png'}}"></image>
