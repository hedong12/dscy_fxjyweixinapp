    </view>
    <view class="item-center" >
      <view class="tabbar-image" >
        <image  src="{{active == 2 ? 'https://yyker-one-stand.oss-cn-beijing.aliyuncs.com/yimai_photos/login/dscy_prod_select.png':'https://yyker-one-stand.oss-cn-beijing.aliyuncs.com/yimai_photos/login/dscy_prod_select2.png'}}"></image>
